{
  "name": "whiskybot",
  "version": "0.2.16",
  "description": "Telegram Bot",
  "main": "src/index.ts",
  "repository": "git@github.com:HackerLacker/whiskyBot.git",
  "author": "Vladislav <ghoulyaev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "bun ./src/index.ts",
    "build": "tsc",
    "watch": "bun --watch ./src/index.ts",
    "lint": "eslint --fix . --ext .ts",
    "prettier": "prettier ./src/ --write",
    "format": "bun lint && bun prettier",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:studio": "prisma studio",
    "prepare": "husky install",
    "test": "bun test",
    "test:watch": "bun --watch test"
  },
  "dependencies": {
    "@prisma/client": "5.7.1",
    "husky": "8.0.3",
    "mysql2": "3.6.5",
    "telegraf": "4.15.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-ts": "1.5.1",
    "@telegraf/types": "6.9.1",
    "@types/jest": "29.5.11",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "bun-types": "1.0.21",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.1",
    "jest": "29.7.0",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "prisma": "5.7.1",
    "ts-jest": "29.1.1",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "*.ts": "bun format"
  }
}
